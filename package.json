{
  "name": "ifr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "sass src/sass/:public/css/ --no-source-map --style=compressed",
    "build:html": "eleventy",
    "dev": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "sass --watch src/sass/:src/css/ --no-source-map --style=compressed",
    "watch:html": "eleventy --serve",
    "convert:webp": "node convert-to-webp.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "markdown-it": "^14.1.0",
    "netlify-cms-media-library-cloudinary": "^1.3.10",
    "npm-run-all": "^4.1.5",
    "sass": "^1.91.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2"
  }
}
